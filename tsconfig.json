{
  "compilerOptions": {
      "target": "es2021",
      "lib": [
          "es2021"
      ],
      "module": "commonjs",
      "rootDir": "./src",
      "moduleResolution": "node",
      "outDir": "./dist",
      "removeComments": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "skipLibCheck": true
  }
}